if [ $# -eq 0 ]
  then
    echo "$0 FILE KEY"
    echo "copies the value associated with KEY from FILE file to pasteboard"
    exit
fi

FILE=$1
NAME=$2

if [ ! -f $FILE ]; then
    echo "file '$FILE' not found"
    exit
fi

openssl enc -aes-256-cbc -d -in $FILE | grep $NAME | sed 's/^[^ ]* //g' | pbcopy
